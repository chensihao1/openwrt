/dts-v1/;

#include "mt7621_elecom_wrc-gst.dtsi"

/ {
	compatible = "elecom,wrc-2533gst", "mediatek,mt7621-soc";
	model = "ELECOM WRC-2533GST";
};

&gmac0 {
	mtd-mac-address = <&factory 0xe000>;
};

&wan {
	mtd-mac-address = <&factory 0xe006>;
};

&partitions {
	partition@50000 {
		compatible = "denx,uimage";
		label = "firmware";
		reg = <0x50000 0xb00000>;
	};

	partition@b50000 {
		label = "tm_pattern";
		reg = <0xb50000 0x380000>;
		read-only;
	};

	partition@ed0000 {
		label = "tm_key";
		reg = <0xed0000 0x80000>;
		read-only;
	};

	partition@f50000 {
		label = "art_block";
		reg = <0xf50000 0x30000>;
		read-only;
	};

	partition@f80000 {
		label = "user_data";
		reg = <0xf80000 0x80000>;
		read-only;
	};
};

&pcie0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0000>;
		ieee80211-freq-limit = <2400000 2500000>;

		led {
			led-sources = <0>;
			led-active-low;
		};
	};
};

&pcie1 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		ieee80211-freq-limit = <5000000 6000000>;

		led {
			led-sources = <0>;
			led-active-low;
		};
	};
};

&state_default {
	gpio {
		groups = "uart3", "jtag", "wdt", "sdhci";
		function = "gpio";
	};
};
