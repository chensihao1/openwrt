/dts-v1/;

#include "mt7621_elecom_wrc-gst.dtsi"

/ {
	compatible = "elecom,wrc-1167gst2", "mediatek,mt7621-soc";
	model = "ELECOM WRC-1167GST2";
};

&leds {
	wlan2g {
		label = "wrc-gst:white:wlan2g";
		gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
	};

	wlan5g {
		label = "wrc-gst:white:wlan5g";
		gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
	};
};

&gmac0 {
	mtd-mac-address = <&factory 0xe000>;
};

&wan {
	mtd-mac-address = <&factory 0xe006>;
};

&partitions {
	partition@50000 {
		compatible = "denx,uimage";
		label = "firmware";
		reg = <0x50000 0x1800000>;
	};

	partition@1850000 {
		label = "tm_pattern";
		reg = <0x1850000 0x400000>;
		read-only;
	};

	partition@1c50000 {
		label = "tm_key";
		reg = <0x1c50000 0x100000>;
		read-only;
	};

	partition@1d50000 {
		label = "nvram";
		reg = <0x1d50000 0xb0000>;
		read-only;
	};

	partition@1e00000 {
		label = "user_data";
		reg = <0x1e00000 0x200000>;
		read-only;
	};
};

&pcie0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0>;
		mtd-mac-address = <&factory 0xe006>;
		mtd-mac-address-increment = <1>;
	};
};

&state_default {
	gpio {
		groups = "i2c", "uart3", "jtag", "wdt", "sdhci";
		function = "gpio";
	};
};
