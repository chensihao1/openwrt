/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9132.dtsi"

/ {
	aliases {
		led-boot = &diag;
		led-failsafe = &diag;
		led-upgrade = &diag;
	};

	flash@1e000000 {
		compatible = "cfi-flash";
		reg = <0x1e000000 0x2000000>;
		bank-width = <2>;
		device-width = <2>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0000000 0x0040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x0040000 0x0020000>;
			};

			partition@60000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x0060000 0x1f60000>;
			};

			partition@1fc0000 {
				label = "user_property";
				reg = <0x1fc0000 0x0020000>;
				read-only;
			};

			art: partition@1fe0000 {
				label = "art";
				reg = <0x1fe0000 0x020000>;
				read-only;
			};
		};
	};

	leds {
		compatible = "gpio-leds";

		usb {
			label = "wzr-hp-g30xnh:blue:usb";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			trigger-sources = <&hub_port>;
			linux,default-trigger = "usbport";
		};

		diag: diag {
			label = "wzr-hp-g30xnh:red:diag";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		wireless {
			label = "wzr-hp-g30xnh:green:wireless";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		security {
			label = "wzr-hp-g30xnh:amber:security";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		router {
			label = "wzr-hp-g30xnh:green:router";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};
	};

	/*
	 * all keys are connected to NXP 74HC153 multiplexer, the driver for it
	 * is not ported to ath79 from ar71xx yet
	 */

	rtl8366: rtl8366 {
		gpio-sda = <&gpio 19 GPIO_ACTIVE_HIGH>;
		gpio-sck = <&gpio 20 GPIO_ACTIVE_HIGH>;

		mdio-bus {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			phy-mask = <0x10>;

			phy4: ethernet-phy@4 {
				reg = <4>;
				phy-mode = "rgmii";
			};
		};
	};
};

&eth0 {
	status = "okay";

	phy-mode = "rgmii";
	mtd-mac-address = <&art 0x1120c>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&eth1 {
	status = "okay";

	phy-mode = "rgmii";
	mtd-mac-address = <&art 0x1120c>;
	mtd-mac-address-increment = <1>;

	phy-handle = <&phy4>;
};

&uart {
	status = "okay";
};

&usb {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x11000>;
	mtd-mac-address = <&art 0x1120c>;
};
